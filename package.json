{
  "name": "autochangelog",
  "version": "0.3.1",
  "description": "Automatic generation of changelogs using the gitlab api + nodejs",
  "main": "lib/autochangelog",
  "preferGlobal": "true",
  "bin": {
    "autochangelog": "lib/autochangelog.js"
  },
  "scripts": {
    "test": "node_modules/.bin/jshint --reporter=node_modules/jshint-stylish lib/autochangelog.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:roshanca/gitlab-autochangelog.git"
  },
  "keywords": [
    "gitlab",
    "changelog"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": {
    "name": "Roshan Wu",
    "email": "wwj1983@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "prompt": "^0.2.14",
    "q": "^1.4.1",
    "request": "^2.58.0",
    "semver": "^4.3.6",
    "yargs": "^3.12.0"
  },
  "devDependencies": {
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1"
  }
}
